{
  "name": "sajda",
  "private": true,
  "version": "0.0.3",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "tsc && vite build",
    "preview": "vite preview",
    "tauri": "tauri",
    "test": "vitest run",
    "test:watch": "vitest",
    "test:coverage": "vitest run --coverage",
    "test:ui": "vitest --ui",
    "test:rust": "cd src-tauri && cargo test",
    "test:all": "npm test && npm run test:rust",
    "sourcemap:inject": "posthog-cli sourcemap inject --directory ./dist/assets --project sajda --version $npm_package_version",
    "sourcemap:upload": "posthog-cli sourcemap upload --directory ./dist/assets --host https://eu.posthog.com",
    "sourcemap:release": "npm run sourcemap:inject && npm run sourcemap:upload"
  },
  "dependencies": {
    "@posthog/react": "^1.7.0",
    "@tauri-apps/api": "^2",
    "@tauri-apps/plugin-autostart": "^2.5.1",
    "@tauri-apps/plugin-geolocation": "^2.3.2",
    "@tauri-apps/plugin-notification": "^2.3.3",
    "@tauri-apps/plugin-opener": "^2",
    "@tauri-apps/plugin-positioner": "^2.3.1",
    "@tauri-apps/plugin-store": "^2.4.2",
    "axios": "^1.13.2",
    "clsx": "^2.1.1",
    "date-fns": "^4.1.0",
    "framer-motion": "^12.28.1",
    "lucide-react": "^0.562.0",
    "posthog-js": "^1.335.2",
    "react": "^19.1.0",
    "react-dom": "^19.1.0",
    "tailwind-merge": "^3.4.0",
    "zustand": "^5.0.10"
  },
  "devDependencies": {
    "@posthog/cli": "^0.5.25",
    "@tauri-apps/cli": "^2",
    "@testing-library/jest-dom": "^6.6.3",
    "@testing-library/react": "^16.3.0",
    "@testing-library/user-event": "^14.6.1",
    "@types/react": "^19.1.8",
    "@types/react-dom": "^19.1.6",
    "@vitejs/plugin-react": "^4.6.0",
    "@vitest/coverage-v8": "^3.1.4",
    "autoprefixer": "^10.4.23",
    "jsdom": "^26.1.0",
    "postcss": "^8.5.6",
    "tailwindcss": "^3.4.17",
    "typescript": "~5.8.3",
    "vite": "^7.0.4",
    "vitest": "^3.1.4"
  }
}
